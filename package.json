{
  "name": "bookstore",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "start": "nodemon app/app.js",
    "lint": "./node_modules/.bin/eslint 'app.js' 'route.js' 'config.js' 'externalServerUrl.js' 'app/**/*.js' -f table --fix",
    "test:coverage": "NODE_ENV=test nyc npm run test:unit",
    "test:unit": "node specs/_run.js",
    "test": "npm run test:unit",
    "test:lint": "eslint -c specs/.eslintrc.js 'specs/**/*.spec.js' -f table table --fix"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "angular": "^1.6.2",
    "angular-route": "^1.6.2",
    "body-parser": "^1.16.1",
    "bootstrap": "^3.3.7",
    "dotenv": "^4.0.0",
    "express": "^4.14.1",
    "jasmine": "^2.7.0",
    "jasmine-console-reporter": "^1.2.7",
    "moment": "^2.18.1",
    "mongoose": "^4.8.3",
    "nyc": "^11.1.0",
    "request": "^2.81.0",
    "request-promise": "^4.2.1",
    "url-template": "^2.0.8",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "eslint": "^4.3.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1"
  },
  "nyc": {
    "include": [
      "app/server/**/*.js"
    ],
    "exclude": [
      "spec/**/*.spec.js"
    ],
    "reporter": [
      "lcov",
      "text",
      "text-summary"
    ],
    "cache": false,
    "lines": 95,
    "statements": 95,
    "functions": 95,
    "branches": 90
  }
}
